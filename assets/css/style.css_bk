@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,800,700|Roboto:400,900,700,500,300,100|Lato:400,100,300,700,900|Oswald:400,300,700|Raleway:400,100,200,300,900,800,700,600,500|Cuprum:400,700|PT+Sans:400,700);

body{font-family: Open Sans,Arial,Helvetica,sans-serif; margin:0; height:auto !important; font-size:13px;  color: #474747; line-height: 1.6; background:url(../images/main_bg.jpg)}
.gray_bg{ background:#F9F7F8;}
a:focus,a:hover{ text-decoration:none; outline:none;}

img{ max-width:100%; height:auto;}

h1,h2,h3,h4,h5,h6,p,ul,li{ padding:0; margin:0; list-style:none;}
h1,h2,h3,h4,h5,h6{font-family: Lora,sans-serif;}
h3 {   font-size: 24px;}
h4 {  font-size: 18px;}
h1{ font-family:Arial, Helvetica, sans-serif; font-size:30px; font-weight:600;}
p{ font-family:Open Sans; margin: 0 0 10px; color: #5b5b5b; font-size: 13px;}
a{ color:#808080;}
a:hover{ color:#E85656;}
img{ max-width:100%;}
h3{color: #d10000; font-size:26px; line-height: 28px;  margin: 20px 0;     padding: 0 0 6px; position:relative;}


.top_header_outer{ float:left; width:100%;position:relative; background:url(../images/banner_header_bg.jpg) no-repeat top center; }
/*.top_header_outer.home{min-height:1103px;}
*/header{ float:left; width:100%; left:0}
/*=======================================================
			TOP HEADER INNER
========================================================*/
.top_header_inner{ float:left; width:100%; background:url(../images/header1_bg.png);  height: 60px;    padding: 17px 0;}
.top_header_inner,.top_header_inner a{ color:#fff; font-size:17px; font-weight:600}
.top_header_inner a.tel_no i {border: 2px solid #fff;border-radius: 50%;    float: left;    font-size: 13px;    margin: 4px 6px 0 0;    padding: 2px 4px;}
.social_icon{ float:right;}
.social_icon li{ float:left;}
.social_icon li a i{ color:#BD1A1D; background:#fff; font-size:16px; font-weight:600; width:25px; height:25px; text-align:center; line-height:26px; border-radius:50%;}
.social_icon > li { margin: 0 0 0 8px;}
.top_header_inner .col-md-4.text-center { font-size: 14px;}
.top_header_outer >img{ width:100%;}
/*=======================================================
			BOTTOM HEADER INNER
========================================================*/

.bottom_header_inner { float: left; width:100%; background:url(../images/header2_bg.png); padding: 21px 0; }
.bottom_header_inner ul { float:right;margin: 30px 35px 0 0;}
.bottom_header_inner ul li{ float:left; margin-left:50px; position:relative;}
.bottom_header_inner ul li a {padding: 6px 8px; display:block;font-family: 'PT Sans', sans-serif;  font-size: 13px;    text-decoration: underline;}
.watch_img{ float:right; margin:22px 0 0;}
.bottom_header_inner ul li:first-child:after{ position:absolute; content:""; width:1px; height:15px; background:#808080; right: -25px;  top: 10px; }
.bottom_header_inner ul ul li:first-child:after{ background:none;}
.bottom_header_inner i {margin: 0 8px 0 0;	background: #808080 ; border: 1px solid #fff;    border-radius: 50%;    box-shadow: 0 0 0 1.5px #808080;  color: #fff;  font-size: 11px;    height: 17px;    line-height: 15px;    width: 17px;}
.bottom_header_inner .member_form{ border-radius:3px; width:302px; padding:10px 15px; background:#fff; position:absolute; right:0; z-index:99999; margin:0}
.bottom_header_inner .member_form li{ margin:0; float:left; width:100%;}
.bottom_header_inner .member_form li label{ font-size:12px; font-weight:600; display:block; float:left; width:100%;}
.bottom_header_inner .member_form li input[type=text],.bottom_header_inner .member_form li input[type=password]{ border: 1px solid #ccc; border-radius: 3px;    box-shadow: 0 5px 6px #ededed inset;    height: 25px;    padding: 5px;    width: 100%;}
.member_form input[type="checkbox"] {  float: left;    margin: 5px 9px 0 0;}
.btn_panel { margin: 15px 0 0; float: left;    width: 100%;}
.btn_panel > a {  float: right;}
.btn_panel > a:first-child {  float: left;}
.btn_panel > span {  float: left;    margin: 5px 0 0 20px;}
.bottom_header_inner ul li a.for_grt{ float:right; text-decoration:none;}
.bottom_header_inner ul ul{ display:none;}
.bottom_header_inner ul li:hover ul{ display:block;}
/*=======================================================
			NAVIGATION
========================================================*/

.navigation_outer ul { text-align: center; float:none;}
.navigation_outer ul li {position:relative;float:none;color:#666666;display:inline-block;font-size: 18px;}
.navbar{ min-height:10px; margin:0; border:none;}
.navigation_outer ul li a{  color: #666;  padding: 12px 40px;  margin-left: 1px;}
.navigation_outer ul li:before{content: ""; height: 20px;    left: 0px;    position: absolute;    top: 14px;    width: 1px;background:#666;}
.navigation_outer ul li:first-child:before{ background:none;}
.navigation_outer { background:url(../images/header3_bg.png);  float: left;border-top: 1px solid #9d9d9d; width: 100%;}
.navigation_outer ul li a:hover{ background:#fff; color:#DA2E2E;}
.navigation_outer ul ul{ display:none; }
.navigation_outer ul ul li{ width:100%; margin:0; font-size:13px; padding:5px 10px 5px 30px; background:url("../images/black_arrow.png") no-repeat scroll 10px 8px / 11px auto}
.navigation_outer ul ul li:before{ background:none;}
.navigation_outer ul li:hover ul{ background: #fff;  display: block;    left: 0px;    padding: 10px 20px;    position: absolute;    text-align: left;    width: 230px;}
/*=======================================================
			BANNER
========================================================*/
.banner_outer{ float:left; width:100%; background:url(../images/banner_bg.png); min-height:853px;}
.banner_box{ min-height:653px; background:url(../images/banner_header_bg.jpg) center -357px; }
.banner_box_outer{ min-height:653px; background:url(../images/slider_bg.png); padding:5px; margin:100px 0 80px}

h3::before { background:url(../images/border1.png) no-repeat scroll 0 0;    content: "";    position: absolute;    width: 167px; height:8px; bottom:-5px; left:0}

/*=======================================================
			Bottom footer
========================================================*/
footer{ float:left; width:100%;}

.bottom_footer,.top_footer{ float:left; width:100%;}
/*=======================================================
			top footer
========================================================*/

.top_footer h3{ margin-bottom:30px;}
.top_footer{ background:url(../images/footer_bg.png); border-top:1px solid #ccc; padding:30px 0 40px}
.bottom_footer{ background:#DA2E2E; color:#fff; font-size:17px; padding:15px 0}
.top_footer h4 {color: #5b5b5b; font-size: 14px;    font-weight: 700;}
.top_footer .col-md-4 { min-height:300px;}
.media_panel .video img,.top_footer .col-md-4 > img { width:95%;border: 10px solid transparent;border-radius: 5px;box-shadow: 0 0 0 1px #da2e2e;}
.top_footer .col-md-4:first-child::before{ background:none;}
.top_footer .col-md-4::before { background: #a9a9a9 none repeat scroll 0 0;    content: "";    height: 100%;    position: absolute;    top: 0;    width: 1px; left:-10px;}
.top_footer ul{ padding:0 0 0 25px;}
.top_footer ul li{ font-size:13px; font-weight:600; margin:0 0 15px; position:relative;}
.top_footer ul li:before{ position:absolute; width:10px; height:10px; left:-23px; top:5px; background:#636363; border-radius:50%; content:"";}
/*=======================================================
			Call to panel
========================================================*/


.call_to_panel{ float:left; width:100%; padding:75px 0; border-bottom:1px solid #ccc;}
.call_to_img{ float:left;}
.call_to_content{ float:right;  width: 65%; border-radius: 0 60px 60px 0; background:#fff; position:relative; min-height:102px;}
.call_to_content h2 {margin: 10px 0 0;color: #d10000;font-family: open sans;font-size: 20px;font-weight: 600; text-transform: uppercase;}
.call_to_content a {border: 1px solid #46413E;border-radius: 50%;    display: block;    height: 13px; position:absolute; right:50px;    margin: ; text-align: center;    width: 13px;  bottom: 10px;}
.call_to_content a img{ width:7px;  margin: 1px 0 0;}
.call_to_content:before{ background:url(../images/img6.png) no-repeat; position:absolute; content:""; width:34px; height:127px; left: -34px; top: -15px;}
.call_to_content p {
    line-height: 17px;
    margin: 5px 45px 0 0;
}



/*=======================================================
		REGISTRATION PANEL
========================================================*/

.reg_tab_panel{ float:left; width:100%}
.reg_tab_panel .nav.nav-tabs {  float: left;  width: 25%;}
.reg_tab_panel .nav.nav-tabs li{ width:100%; float:left; text-align:right; border-top:1px solid #d10000; margin:0}
.reg_tab_panel .nav.nav-tabs li:first-child{ border:none;}
 .nav.nav-tabs li a{background:#FDEB6B; padding:30px 20px; color:#46413E; font-size:20px; font-weight:600; display:block; float:left;width:100%;  line-height: 52px;}
.reg_tab_panel .nav.nav-tabs li a img{ float:left;}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus{background:#FDEB6B;  color:#d10000; }
.nav-tabs > li > a:hover{ border-color:transparent}
#myTabContent { background: #46413e;  border-radius: 0 0 5px 5px; padding: 30px 20px 23px; text-align:right;   float: left;    min-height: 230px;    width: 75%; color:#fff;}
.tab-pane table{ width:100%;}
.tab-pane li{ display: block; float: left;    margin: 0 0 13px;    width: 33.33%;}
.tab-pane li select{ width:159px; color: #ccc;  font-size: 11px; margin-left:2px;}
.tab-pane li label { font-size: 13px;  font-weight: 500;}
.tab-pane li select.dd,.tab-pane li select.mm,.tab-pane li select.yy{ width:49px;} 
.input_text {color: #868686;    height: 25px;    padding: 5px;    width: 160px;}
.submit_button { background: #ffe001;    border: medium none;    border-radius: 3px;    color: #d10000;    font-size: 20px;    font-weight: 500;    padding: 5px 0;    text-transform: uppercase;   width: 157px;}
.tab-pane li td input[type=radio]{ margin:0 5px 0 0;}
.nav-tabs > li.active > a:before { content:""; position:absolute; right:-10px; top:50px;width: 0; height: 0; 	border-top: 10px solid transparent;	border-bottom: 10px solid transparent;		border-left: 10px solid #FDEB6B;}
.tab-pane li select.year1, .tab-pane li select.year2 { width: 80px;}
#profile ul{ margin:30px 0 0}


/*=======================================================
		Services
========================================================*/

.services_panel{ float:left; width:100%; padding:20px 0 40px}
.featured_list > li { padding:13px;  background: #fff ; float:left; width:100%;    border: 1px solid #ccc;    border-radius: 5px; margin: 0 0 20px;}
.featured_list > li .featured_box {height: 117px;position:relative;
    overflow: hidden;
    border: 1px solid #ccc;
    float: left;
    width: 100%;
}
.featured_img{ float:left; width: 35%;}
.featured_content{ float:right; width:60%; }
.featured_content:before{ position:absolute; width:30px; height:30px; background:url(../images/corner_2.png) no-repeat; content:""; right:5px; top:5px;}
.featured_content:after{ position:absolute; width:30px; height:30px; background:url(../images/corner_1.png) no-repeat; content:""; right:5px; bottom:5px;}

.featured_content p{ margin:0}
.success_box h2,.featured_content h2 { color: #da2e2e;    font-size: 18px;    margin: 15px 0 4px;}
.success_box_inner a img,.featured_content img { width: 7px;}
.featured_content a {border: 1px solid #da2e2e;    border-radius: 50%;    bottom: 15px;    padding: 2px 3px 2px 4px;    position: absolute;    right: 40px;}
.success_box h2{ text-align:center;}
.success_box { background: #fff none repeat scroll 0 0;    border-radius: 5px;    padding: 15px;}
.success_box_inner { border: 1px solid #ccc;}
.success_box_inner >img{ width:100%;}
.success_box_inner > p { margin:0; line-height: 18px;    padding: 0 25px;    text-align: center;}
.success_box_inner a { border: 1px solid #da2e2e;border-radius: 50%;    display: block;    height: 15px;    margin: 10px auto 15px;    text-align: center;    width: 15px;}
.success_box_inner a img { margin: 2px 0 0;}

/*=======================================================
		About page
========================================================*/



.about_content_panel{ float:left; width:100%; border-top:1px solid #ccc; padding:50px 0;}
.about_content_panel img.pull-left{ margin:0 10px 0 0}
.about_inner {  background: #fcecec none repeat scroll 0 0; float:left; width:100%;    border: 1px solid #ccc;}
.about_inner h1 {margin: 0 0 30px;background: #f0898a none repeat scroll 0 0;    color: #fff;    font-family: open sans;    font-size: 22px;    font-weight: 700;    padding: 10px;    text-align: center;}
.about_inner > p { line-height:24px; float: right;    padding: 0 20px 15px;    width: 44%;}


.update_panel{ float:left; width:100%; padding:10px 0 70px}
.update_inner{ float:left; width:100%; border:1px solid #ccc; background:#FEDD76; min-height:165px; padding:20px;}
.update_panel h4 {color: #6d6d6d;    font-size: 19px;  margin: 0 0 15px;font-weight: 600;    text-transform: capitalize;}
.update_inner > img { float: left;  margin: 0 10px 0 0;}
.update_inner a{ color:#DA2E2E;}
.update_inner > p { line-height: 25px;    margin: 8px 0 0;}
.go_button{ background:url(../images/go_btn.jpg); color:#fff; color:#fff; border:none; width:38px; height:38px; box-shadow: 0 0 7px #c8c8c8;}
.update_inner .input_text {
    height: 38px;
    margin: 0 10px 0 0;
    width: 168px;
}
/*=======================================================
		media and event page
========================================================*/


.media_panel{ float:left; width:100%;padding:50px 0}
.media_panel h4{ color:#fff; font-size:22px; background:rgba(236, 128, 127, 0.7); text-align:center; padding:10px 0; font-weight:600; font-family: 'PT Sans', sans-serif; }
.pad0{ padding:0}
.media_panel h5{ font-weight:600; }
.media_panel li{ float:left; width:100%; margin-bottom:20px; padding:40px 0; border-bottom:1px solid #ccc;}
.media_panel li:last-child{ border:none; padding-bottom:0}



.title_panel{ float:left; width:100%; padding: 60px 0;}
.add_pic_panel .col-md-4 ul li {list-style: outside none disc;}
.add_pic_panel .col-md-4 ul { padding: 15px 0 0 20px;}
.title_panel h1{ float:left; color:#000; font-size:31px; font-weight:600}
.title_panel a{ color:#d31f1f; float:right;}
.add_pic_cont_outer .add_pic_cont,.add_pic_cont_outer  a,.add_pic_cont_outer { float:left;}
.add_pic_panel{ float:left; width:100%; padding:0 0 50px;}
.add_pic_cont_outer {  position: relative;    width: 100%;}
.add_pic_panel .col-md-8 ul li { float: left;    margin: 0 0 30px;    width: 100%;}
.add_pic_panel .col-md-8 ul li p{ text-align:center;}
.add_pic_panel .col-md-8 > ul {float: left;  margin: 0 0 0 50px;    width: 60%;}
.add_pic_cont_outer > a{position: absolute; right:-50px}
.add_pic_cont_outer > a:first-child { left: -50px; right:auto;}
.add_pic_cont {background: rgba(255, 205, 4, 0.5) ; color: #cc1f1f; font-family: arial;    font-size: 22px;    font-weight: 600;    height: 100px;    line-height: 24px;    padding: 25px 180px 0 70px;    width: 100%;}


/*=======================================================
		contact page
========================================================*/

.main_container{ padding:50px 0}
.contact_full,.contact_right,.contact_left,.main_container{ float:left; width:100%;}
.contact_left{ border:1px solid #ccc; border-top:none}
.contact_left > h2 { background: #f0898a none repeat scroll 0 0; color: #fff;    font-family: arial;    font-size: 22px;    font-weight: 600;    padding: 10px;    text-align: center;}
.contact_left_inner{ float:left; width:100%; padding:30px 50px 30px 30px;}
.contact_left_inner p{ letter-spacing:0.3px; line-height:24px;}
.contact_left{ min-height:600px;}
.contact_right{ background:rgba(255, 255, 255, 0.5) ; border-radius:10px; padding:20px; border:1px solid #ccc;}
.contact_right h2 { border-bottom: 1px solid #ccc;    color: #d10000;    font-family: Arial,Helvetica,sans-serif;    font-size: 20px;    font-weight: 600;    margin-left: -20px;    padding:0 0 15px;    text-indent: 20px;}
.contact_right table{ width:100%; margin:20px 0}
.contact_right .input_type_text { position:relative;  border: 1px solid #ccc;    border-radius: 5px;    height: 45px;   padding: 5px 5px 5px 55px;    width: 100%; font-size:15px;}
.contact_right table td { padding: 10px;}
.contact_right img {border-right: 1px solid #ccc;    height: 44px;    padding: 12px;    position: absolute;    width: 48px;    z-index: 9999;}
.contact_right .text_area {border: 1px solid #ccc;    border-radius: 5px;    font-size: 15px;    height: 170px;    padding: 5px;    resize: none;    text-transform: capitalize;    width: 100%;}
.contact_right .send{ background:url(../images/contact-us-btn.jpg); color:#d10000; border:none; width:100%; font-size:20px; font-weight:600; text-transform:capitalize; border:1px solid #ccc; padding:9px;}
.contact_full{border:1px solid #ccc; margin:30px 0 0; background:#FCECEC;}
.contact_full h2{ background:#F0898A; color:#fff; font-size:22px; font-family:Arial, Helvetica, sans-serif; font-weight:600; text-align:center; text-transform:capitalize; padding:10px;}
.contact_full label {display: block;  font-weight: 500;    text-transform: capitalize;}
.contact_full_inner {
    float: left;
    padding: 20px 50px;
    width: 100%;
}
.contact_full .input_text { border-radius:5px; border:1px solid #ccc; height: 40px;    padding: 5px;    width: 100%;}
.contact_full select { border: 1px solid #ccc; border-radius: 5px;    height: 40px;    padding: 5px;    width: 100%;}
.send_btn1,.send_btn{ display:block; float:right; background:url(../images/button_bg2.jpg); color:#000; border:1px solid #ccc; text-transform:capitalize; font-weight:600; font-size:16px; padding:5px 20px; border-radius:5px; margin: 28px 0 0; box-shadow: 0 0 6px 1px #d4d4d4; }
.contact_full_inner .col-md-4{ margin-bottom:30px;}
