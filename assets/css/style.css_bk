#myTab li.active > a::before { border-left: 10px solid transparent;    border-right: 10px solid transparent;    border-top: 10px solid #ef8b8b;    bottom: -20px;    content: "";    height: 0;    position: absolute;    right: 47%;    top: auto;    width: 0;}

#myTab{ margin:0 !important}
#myTabContent1 {  background: #fcf4f2; float:left;    border: 1px solid #ccc;    padding: 20px 0 0; border-top:none}
.search_panel_bottom { background: #fcf4f2;    border: 1px solid #ccc;    float: left;    margin: 40px 0 0;    width: 100%; border-top:none}
