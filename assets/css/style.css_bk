.match_box ul{ float:left; width:100%; margin:20px 0 0}
.profile_info h5 {
    float: left;
    font-weight: 600;
    margin: 20px 0 10px;
    width: 100%;
}

.match_box li{ float:left; width:100%; padding:0 15px; margin-bottom:30px;}
.match_img { width: 35%;}
.match_cross { width: 4%;}
.match_box li div{ float:left;}
