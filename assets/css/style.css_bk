.search_panel_bottom span { margin: 0 10px;}
.search_panel_bottom  table td:first-child { font-size: 15px;    font-weight: 600;}
.search_panel_bottom select { border: 1px solid #ccc;    color: #9c9c9c;    width: 180px;}
.search_text { border: 1px solid #ccc;    width: 50px;}

.send_btn1,.search_panel_bottom .send_btn{ display:inline-block; float:none;}
.send_btn1{ background:url(../images/button_bg1.jpg) bottom #FEFEFE; border-color:#666;}
.col-md-3 .update_inner.mar_175{ margin:175px 0 0}
.col-md-3 .update_inner h4{ font-family:Arial, Helvetica, sans-serif; font-size:20px; font-weight:600; margin:0 0 15px;}
/*=======================================================
		reguler search page
========================================================*/


.search_panel_inner{ float:left; width:100%; background:#FCECEC; min-height:400px;}
.search_panel_inner form{ float:left; width:100%; margin-bottom:10px;}
